---
modules:
  node_exporter:
    method: http
    http:
       port: 9100
{% if 'postgresql_servers' in group_name %}
  postgres_exporter:
    method: http
    http:
      port: 9187
{% endif %}
{% for exp in prometheus_exporter_exporters|default([]) %}
  {{ exp.name }}:
    method: http
    http:
      port: {{ exp.httpport }}
{% endfor %}

